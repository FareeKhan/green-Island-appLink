{
  "routes": [
    {
      "src": "/.well-known/apple-app-site-association",
      "dest": "/public/.well-known/apple-app-site-association",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/.well-known/(.*)",
      "dest": "/public/.well-known/$1"
    },
    {
      "src": "/productDetails/(.*)",
      "status": 302,
      "headers": {
        "Location": "intent://green-island-app-link.vercel.app/productDetails/$1#Intent;scheme=https;package=com.greenislandapp;S.browser_fallback_url=https://play.google.com/store/apps/details?id=com.greenislandapp;end;"
      },
      "continue": true
    },
    {
      "src": "/productDetails/(.*)",
      "status": 302,
      "headers": {
        "Location": "https://apps.apple.com/app/id6746166638"
      }
    }
  ]
}
